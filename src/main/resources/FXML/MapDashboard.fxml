<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.control.TreeTableColumn?>
<?import javafx.scene.control.TreeTableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<Pane prefHeight="800.0" prefWidth="1200.0" stylesheets="@../CSS/DashboardCSS.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.cs3733.d22.teamV.controllers.MapDashboardController">
   <children>
      <VBox prefHeight="800.0" prefWidth="1200.0">
         <children>
            <Pane id="Menu" fx:id="menu" minHeight="-Infinity" prefHeight="138.0" prefWidth="596.0" style="-fx-background-color: #012D5Aff;">
               <children>
                  <ImageView fitHeight="88.0" fitWidth="122.0" layoutX="29.0" layoutY="22.0" onMouseClicked="#switchToHome" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../Brigham_and_Womens_Hospital_logo.png" />
                     </image>
                  </ImageView>
                  <Label layoutX="131.0" layoutY="27.0" prefHeight="36.0" prefWidth="349.0" stylesheets="@../CSS/DashboardCSS.css" text="Map Dashboard" textFill="WHITE">
                     <font>
                        <Font name="System Bold" size="51.0" />
                     </font>
                  </Label>
               </children>
            </Pane>
            <HBox fx:id="hBox" prefHeight="714.0" prefWidth="1200.0">
               <children>
                  <VBox fx:id="leftVBox" prefHeight="716.0" prefWidth="404.0" spacing="10.0" style="-fx-background-color: white;">
                     <children>
                        <TitledPane animated="false" text="Side View Map">
                           <content>
                              <Pane fx:id="sideViewPane" prefHeight="790.0" prefWidth="400.0">
                                 <children>
                                    <ImageView fx:id="sideViewImage" fitHeight="591.0" fitWidth="390.0" layoutX="8.0" layoutY="5.0" pickOnBounds="true" preserveRatio="true">
                                       <image>
                                          <Image url="@../SideView.PNG" />
                                       </image>
                                    </ImageView>
                                 </children>
                              </Pane>
                           </content>
                        </TitledPane>
                     </children>
                  </VBox>
                  <VBox fx:id="centerVBox" prefHeight="716.0" prefWidth="434.0" spacing="10.0" style="-fx-background-color: white; -fx-padding: 20;">
                     <children>
                        <TitledPane animated="false" prefHeight="304.0" prefWidth="392.0" style="-fx-background-color: #000000;" text="Detailed Information">
                           <content>
                              <TabPane prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE">
                                <tabs>
                                  <Tab text="Equipment">
                                       <content>
                                          <TreeTableView prefHeight="200.0" prefWidth="200.0">
                                            <columns>
                                              <TreeTableColumn prefWidth="75.0" text="C1" />
                                              <TreeTableColumn prefWidth="75.0" text="C2" />
                                            </columns>
                                          </TreeTableView>
                                       </content>
                                  </Tab>
                                  <Tab text="Patients">
                                       <content>
                                          <TreeTableView prefHeight="200.0" prefWidth="200.0">
                                            <columns>
                                              <TreeTableColumn prefWidth="75.0" text="C1" />
                                              <TreeTableColumn prefWidth="75.0" text="C2" />
                                            </columns>
                                          </TreeTableView>
                                       </content>
                                  </Tab>
                                    <Tab text="Locations">
                                       <content>
                                          <TreeTableView prefHeight="200.0" prefWidth="200.0">
                                            <columns>
                                              <TreeTableColumn prefWidth="75.0" text="C1" />
                                              <TreeTableColumn prefWidth="75.0" text="C2" />
                                            </columns>
                                          </TreeTableView>
                                       </content>
                                    </Tab>
                                </tabs>
                              </TabPane>
                           </content>
                        </TitledPane>
                        <TitledPane animated="false" prefHeight="293.0" prefWidth="412.0" text="Counts">
                           <content>
                              <TreeTableView prefHeight="200.0" prefWidth="200.0">
                                <columns>
                                  <TreeTableColumn prefWidth="75.0" text="C1" />
                                  <TreeTableColumn prefWidth="75.0" text="C2" />
                                </columns>
                              </TreeTableView>
                           </content>
                        </TitledPane>
                     </children>
                  </VBox>
                  <VBox fx:id="rightVBox" prefHeight="716.0" prefWidth="370.0" style="-fx-background-color: white;">
                     <children>
                        <TitledPane animated="false" prefHeight="368.0" prefWidth="368.0" text="Interactive Map">
                           <content>
                              <ImageView fitHeight="365.0" fitWidth="365.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../1st%20Floor.png" />
                                 </image>
                              </ImageView>
                           </content>
                        </TitledPane>
                        <TitledPane animated="false" prefHeight="314.0" prefWidth="368.0" style="-fx-padding: 10;" text="Alerts">
                           <content>
                              <TreeTableView prefHeight="200.0" prefWidth="200.0">
                                <columns>
                                  <TreeTableColumn prefWidth="75.0" text="C1" />
                                  <TreeTableColumn prefWidth="75.0" text="C2" />
                                </columns>
                              </TreeTableView>
                           </content>
                        </TitledPane>
                     </children>
                  </VBox>
               </children>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="1200.0">
               <children>
                  <Button fx:id="ll2" mnemonicParsing="false" text="LL2" />
                  <Button fx:id="ll1" mnemonicParsing="false" text="LL1" />
                  <Button fx:id="f1" mnemonicParsing="false" text="1" />
                  <Button fx:id="f2" mnemonicParsing="false" text="2" />
                  <Button fx:id="f3" mnemonicParsing="false" text="3" />
                  <Button fx:id="f4" mnemonicParsing="false" text="4" />
                  <Button fx:id="f5" mnemonicParsing="false" text="5" />
               </children>
               <padding>
                  <Insets right="100.0" top="10.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
   </children>
</Pane>
